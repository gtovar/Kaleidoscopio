<!DOCTYPE html>
<html>
<head>
<%= ('<meta property="og:title" content="Kaleydoscopio | ' + curso_path(@course).gsub("/curso/","") + '" />').html_safe %>
<%= ('<meta property="og:type" content="Clases" />').html_safe %>
<%= ('<meta property="og:url" content="'+ curso_url(@course) + '" />').html_safe %>
<%= ('<meta property="og:image" content="' + @course.photo.large.url.to_s + '" />').html_safe %>
<meta property="og:site_name" content="Kaleydoscopio site name" />
<meta property="fm:admins" content="589571332" />
  <title>Kaleydoscopio</title>
  <%= stylesheet_link_tag    "custom", :media => "all" %>
  <%= stylesheet_link_tag    "jquery-ui-1.8.16.custom", :media => "all"%> 
	<%= javascript_include_tag "back-end.js" %>
	<%= javascript_include_tag "jquery-ui-1.8.16.custom.min" %>
	<%= javascript_include_tag "jquery-ui-timepicker-addon" %>
	<%= javascript_include_tag "jquery-ui-sliderAccess" %>
	<%= javascript_include_tag "jquery-ui-timepicker-es" %>


<script type="text/javascript">
$(document).ready(function () {
$('#course_date_time').datetimepicker();
});
  </script>
  <%= csrf_meta_tags %>
<%= render 'layouts/shim' %> 
</head>
<body>
    <%= render 'layouts/header' %>
    <div class="container">
      <%= yield %>
      <!--<%= debug(params) if Rails.env.development? %>-->

    </div>
<br/>
<br/>
  </body>

</html>
